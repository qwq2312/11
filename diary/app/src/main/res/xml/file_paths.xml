<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!--代表外部存储区域的根目录下的文件 Environment.getExternalStorageDirectory()/DCIM/exp2目录-->
    <external-path name="DCIM" path="." />
    <!--代表外部存储区域的根目录下的文件 Environment.getExternalStorageDirectory()/Pictures/exp2目录-->
    <external-path name="Pictures" path="Pictures/exp2" />
    <!--代表app 私有的存储区域 Context.getFilesDir()目录下的images目录 /data/user/0/com.example.exp2/files/images-->
    <files-path name="private_files" path="images" />
    <!--代表app 私有的存储区域 Context.getCacheDir()目录下的images目录 /data/user/0/com.example.exp2/cache/images-->
    <cache-path name="private_cache" path="images" />
    <!--代表app 外部存储区域根目录下的文件 Context.getExternalFilesDir(Environment.DIRECTORY_PICTURES)目录下的Pictures目录-->
    <!--/storage/emulated/0/Android/data/com.example.exp2/files/Pictures-->
    <external-files-path name="external_files" path="Pictures" />
    <!--代表app 外部存储区域根目录下的文件 Context.getExternalCacheDir目录下的images目录-->
    <!--/storage/emulated/0/Android/data/com.example.exp2/cache/images-->
    <external-cache-path name="external_cache" path="images" />

</paths>